{
  "name": "react-testing-sample",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "16.8.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-scripts": "3.0.1",
    "react-transition-group": "^4.2.2",
    "redux": "^3.6.0",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0",
    "validatorjs": "3.14.2",
    "wait-for-expect": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "8.0.9",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "5.16.0",
    "eslint-config-react-tools": "^2.0.5",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "7.14.2",
    "fetch-mock": "^5.12.2",
    "prettier": "^1.18.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 95,
        "lines": 95,
        "statements": 95
      },
      "./src/App.js": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
